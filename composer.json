    "require": {
        "aklump\/loft-lib": "^1.3",
        "guzzlehttp\/guzzle": "^6.3|^7.0",
        "symfony\/yaml": "^3.4.0|^4.0|^5.0",
        "symfony/dom-crawler": "^5.0",
