{
    "name": "aklump/check-pages",
    "type": "library",
    "description": "Very simple QA for websites.",
    "keywords": [],
    "homepage": "https://github.com/aklump/check-pages",
    "license": "BSD-3-Clause",
    "autoload": {
        "files": [
            "includes/runner_functions.inc"
        ],
        "psr-4": {
            "AKlump\\Messaging\\": "stash/Messaging/",
            "AKlump\\CheckPages\\": "src/",
            "AKlump\\CheckPages\\Plugin\\": [
                "plugins/breakpoint/",
                "plugins/count/",
                "plugins/cypress/",
                "plugins/device/",
                "plugins/dom/",
                "plugins/evaluate/",
                "plugins/form/",
                "plugins/header/",
                "plugins/import/",
                "plugins/javascript/",
                "plugins/json_pointer/",
                "plugins/json_schema/",
                "plugins/loop/",
                "plugins/path/",
                "plugins/redirect/",
                "plugins/request/",
                "plugins/sleep/",
                "plugins/style/",
                "plugins/value/",
                "plugins/xpath/"
            ]
        }
    },
    "authors": [
        {
            "name": "Aaron Klump",
            "email": "sourcecode@intheloftstudios.com"
        }
    ],
    "require": {
        "aklump/loft-lib": "^1.3",
        "behat/mink": "^1.8",
        "chrome-php/chrome": "^1.8",
        "dmore/chrome-mink-driver": "^2.7",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "justinrainbow/json-schema": "^5.2",
        "laminas/laminas-xml2json": "^3.2",
        "midweste/objectpath": "^1.0",
        "opis/json-schema": "^2.3",
        "php": ">=7.2",
        "php-jsonpointer/php-jsonpointer": "^4.0",
        "ralouphie/mimey": "^2.1",
        "shanethehat/pretty-xml": "^1.0",
        "spatie/browsershot": "^3.57",
        "symfony/config": "^4.4|^5.4",
        "symfony/console": "^4.4|^5.4",
        "symfony/css-selector": "^3.4|^4.0|^5.0",
        "symfony/dependency-injection": "^4.4|^5.4",
        "symfony/dom-crawler": "^5.0",
        "symfony/event-dispatcher": "^4.4|^5.4",
        "symfony/expression-language": "^4.4|^5.4",
        "symfony/yaml": "^3.4|^4.0|^5.0"
    },
    "version": "0.17.0",
    "bin": [
        "./check_pages",
        "./create_test_directory"
    ],
    "scripts": {
        "post-install-cmd": [
            "./create_test_directory"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "drupal/coder": "^8.3",
        "phpunit/phpunit": "^9.5"
    }
}